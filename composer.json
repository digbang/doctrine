{
	"name": "digbang/doctrine",
	"description": "Tools and configurations to use Doctrine ORM inside Laravel in Digbang.",
	"require": {
		"doctrine/orm": "2.5.*@dev",
		"illuminate/cache": "~4.2",
		"illuminate/config": "~4.2",
		"illuminate/support": "~4.2",
		"illuminate/container": "~4.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"maximebf/debugbar": "~1.10"
	},
	"autoload": {
		"psr-4": {
			"Digbang\\Doctrine\\": "src/"
		}
	},
	"license": "proprietary",
	"minimum-stability": "dev",
	"prefer-stable": true
}
