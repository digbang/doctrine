{
	"name": "digbang/doctrine",
	"description": "Tools and configurations to use Doctrine ORM inside Laravel in Digbang.",
    "license": "proprietary",
    "require": {
        "doctrine/orm":                  "~2.5",
        "illuminate/support":            "~5.1",
        "illuminate/contracts":          "~5.1",
        "mitchellvanw/laravel-doctrine": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit":    "~4.0",
        "maximebf/debugbar":  "~1.10",
        "illuminate/console": "~5.1"
    },
    "autoload": {
        "psr-4": {
            "Digbang\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
	"prefer-stable": true
}
