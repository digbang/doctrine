	"require": {
		"doctrine/orm": "2.5.*@dev",
		"illuminate/support": "~5.0",
		"illuminate/contracts": "~5.0",
		"mitchellvanw/laravel-doctrine": "dev-develop"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"maximebf/debugbar": "~1.10",
		"illuminate/console": "~5.0"
	},
	"autoload": {
		"psr-4": {
			"Digbang\\Doctrine\\": "src/"
		}
	},
	"license": "proprietary",
	"minimum-stability": "dev",
	"prefer-stable": true
}
