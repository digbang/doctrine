{
	"name": "digbang/doctrine",
	"description": "Tools and configurations to use Doctrine ORM inside Laravel in Digbang.",
    "license": "proprietary",
    "require": {
        "doctrine/orm":         "~2.5",
        "illuminate/support":   "~5.1",
        "illuminate/contracts": "~5.1"
    },
    "require-dev": {
        "illuminate/cache":  "~5.1",
        "illuminate/console": "~5.1",
        "maximebf/debugbar": "~1.10",
        "phpunit/phpunit":   "~4.0",
		"illuminate/auth": "^5.1",
		"doctrine/migrations": "^1.0"
    },
    "suggest": {
        "doctrine/migrations": "Use doctrine migrations to generate SQL migration files instead of using the schema tool."
    },
    "autoload": {
        "psr-4": {
            "Digbang\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
